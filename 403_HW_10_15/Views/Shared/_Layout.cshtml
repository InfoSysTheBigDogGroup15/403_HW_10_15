<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
    <title>Index</title>
</head>
<body>
    <div class="container">
        <div class="w3-sidebar w3-light-grey w3-bar-block" style="width:10%">
            <h3 class="w3-bar-item">Menu</h3>
            <a href="@Url.Action("Index", "Stack")"
               class="w3-bar-item w3-button">Stack</a>

            <a href="@Url.Action("Index", "Queue")"
               class="w3-bar-item w3-button">Queue</a>

            <a href="@Url.Action("Index", "Dictionary")"
               class="w3-bar-item w3-button">Dictionary</a>

            <a href="@Url.Action("Exit", "Home")"
               class="w3-bar-item w3-button">Exit</a>
        </div>
        <div style="margin-left:10%">
            <div class="w3-container w3-blue-grey">
                <h1>Dr.Anderson's Data Structure Class</h1>
            </div>
            @RenderBody()
        </div>
        <div class="modal-footer">

        </div>
    </div>
</body>
</html>